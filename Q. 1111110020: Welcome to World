#include <stdio.h>

int main() 
{	char name[20],inventor[20],purpose[20];
 	int memory;
 	float speed;
 	scanf("%s\n",name);
 	scanf("%s\n",inventor);
	scanf("%s\n",purpose);
 	scanf("%d\n",&memory);
 	scanf("%f",&speed);
 	printf("Robot Details\n");
	printf("I am the Robot named=%s\n",name);
 	printf("I was created by %s\n",inventor);
 	printf("I am created for the purpose of %s\n",purpose);
 	printf("My memory space is around %dmb and my speed is %0.1fTB",memory,speed);
 
	return 0;
}
