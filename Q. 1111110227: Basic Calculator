#include <stdio.h>
int main() 
{ char c; 
 int a,b;
scanf("%d\n",&a);
scanf("%c\n",&c);
scanf("%d",&b);

 
 switch(c)
 {
   case'+':
     printf("Answer=%d",a+b);
     break;
   case'-':
     printf("Answer=%d",a-b);
     break;
   case'*':
     printf("Answer=%d",a*b);
     break;
   case'/':
     printf("Answer=%d",a/b);
     break;
   case'%':
     printf("Answer=%d",a%b);
     break;
   default:
     printf("error");
 }

	return 0;
}
